							return
						end

						if not input or input == "" then
							return
						end

						local is_directory = input:sub(-1) == "/"
						local filename = current_dirname .. input

						-- æ—¢å­˜ãƒ‡ã‚£ãƒ¬ã‚¯ãƒˆãƒªãƒ¼ã¨é‡è¤‡
						if vim.fn.isdirectory(filename) == 1 then
							vim.notify(
